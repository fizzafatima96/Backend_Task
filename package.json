{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "start": "ts-node-dev --respawn --transpile-only src/index.ts",
    "clean": "rimraf ./dist",
    "build": "npm run clean && tsc",
    "serve": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "migrate:local": "SET NODE_ENV=test&&npx sequelize-cli db:migrate",
    "migrate:dev": "npx sequelize-cli db:migrate --env dev",
    "migrate:prod": "SET NODE_ENV=production&&npx sequelize db:migrate",
    "seed:local": "SET NODE_ENV=test&&npx sequelize-cli db:seed:all",
    "seed:staging": "SET NODE_ENV=development&&npx sequelize-cli db:seed:all",
    "seed:prod": "SET NODE_ENV=production&&npx sequelize-cli db:seed:all"
  },

  "keywords": [],
  "author": "",
  "license": "ISC",
  
  "dependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.17",
    "archiver": "^7.0.1",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "decimal.js": "^10.4.3",
    "dotenv": "^16.1.4",
    "express": "^4.18.2",
    "express-validation": "^4.1.0",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.45",
    "nanoid": "^3.3.4",
    "nodemailer": "^6.9.7",
    "otp-generator": "^4.0.1",
    "pg": "^8.11.0",
    "qs": "^6.12.0",
    "sequelize": "^6.37.3",
    "serverless-http": "^3.2.0",
    "serverless-prune-plugin": "^2.0.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.540.0",
    "@aws-sdk/s3-request-presigner": "^3.540.0",
    "@types/archiver": "^6.0.2",
    "@types/bcrypt": "^5.0.0",
    "@types/ejs": "^3.1.5",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.14.8",
    "@types/nodemailer": "^6.4.14",
    "@types/sequelize": "^4.28.20",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "nodemon": "^2.0.22",
    "rimraf": "^5.0.5",
